@{
    // ReSharper disable once Razor.LayoutNotResolved From SFA.DAS.Provider.Shared.UI
    Layout = "_Layout";
}

@RenderBody()

@section Styles
{
    @RenderSection("Styles", required: false)
    <link href="/css/site.css" rel="stylesheet" no-cdn />
}

@section breadcrumb
{
    @RenderSection("breadcrumb", required: false)
}

@section Scripts
{
    @RenderSection("Scripts", required: false)
    <script src="libs/accessible-autocomplete/accessible-autocomplete.min.js"></script>
    <script src="/js/site.js" no-cdn></script>
}